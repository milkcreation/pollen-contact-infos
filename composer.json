{
  "name": "pollen-solutions/contact-infos",
  "description": "Contact Infos Component",
  "homepage": "https://github.com/milkcreation/pollen-contact-infos.git",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "contact",
    "informations"
  ],
  "time": "2021-09-14",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Pollen\\ContactInfos\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\ContactInfos\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
